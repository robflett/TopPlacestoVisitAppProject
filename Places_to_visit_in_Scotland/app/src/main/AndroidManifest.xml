<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codeclan.places_to_visit_in_scotland">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/scotland_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/scotland_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpandableActivity" />
        <activity android:name=".addNewPlaceActivity" />
        <activity android:name=".Filter_by_Walking_Activity" />
        <activity android:name=".Filter_by_Food_Activity"></activity>
    </application>

</manifest>